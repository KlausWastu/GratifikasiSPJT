<table id="example2" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Tanggal</th>
      <th>Nama Peminta</th>
      <th>Status Laporan</th>
      <th>Preview</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i=0; i< laporpermintaan.length; i++) {%>
    <tr>
      <td><%= i+1 %></td>
      <td><%= laporpermintaan[i].tglPembuatanLaporan %></td>
      <td><%= laporpermintaan[i].dataPeminta.namaPeminta %></td>
      <td>
        <% if(laporpermintaan[i].statusBaca === false) {%>
        <i class="nav-icon fa fa-eye-slash" aria-hidden="true"></i>
        <span>Belum Ditinjau</span>
        <% } else if(laporpermintaan[i].statusBaca === true) {%>
        <i class="nav-icon fa fa-eye" aria-hidden="true"></i>
        <span>Ditinjau</span>
        <% } %>
      </td>
      <td>
        <a
          href="/riwayatPermintaan/preview/<%= laporpermintaan[i]._id %>"
          class="btn btn-primary btn-sm"
          >lihat</a
        >
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
