<table id="example2" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Tanggal</th>
      <th>Nama Pemberi</th>
      <th>Status Laporan</th>
      <th>Preview</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i=0; i< laporpenerimaan.length; i++) {%>
    <tr>
      <td><%= i+1 %></td>
      <td><%= laporpenerimaan[i].tglPembuatanLaporan %></td>
      <td><%= laporpenerimaan[i].dataPemberi.namaPemberi %></td>
      <td>
        <% if(laporpenerimaan[i].statusBaca === false) {%>
        <i class="nav-icon fa fa-eye-slash" aria-hidden="true"></i>
        <span>Belum Ditinjau</span>
        <% } else if(laporpenerimaan[i].statusBaca === true) {%>
        <i class="nav-icon fas fa-eye" aria-hidden="true"></i>
        <span>Ditinjau</span>
        <% } %>
      </td>
      <td>
        <a
          href="/riwayatPenerimaan/preview/<%= laporpenerimaan[i]._id %>"
          class="btn btn-primary btn-sm"
          >lihat</a
        >
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
